    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "baseUrl": "",
    "paths": {
      "rxjs": [ "jspm_packages/npm/rxjs@6.2.1" ],
      "rxjs/*": [ "jspm_packages/npm/rxjs@6.2.1/*" ],
